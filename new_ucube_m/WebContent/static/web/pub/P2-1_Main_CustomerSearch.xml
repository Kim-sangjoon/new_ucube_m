<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head meta_programName="메인화면 - 고객조회">
    	<w2:type>DEFAULT</w2:type>
        <w2:buildDate/>
        <xf:model>
            <xf:instance>
                <data xmlns=""/>
            </xf:instance>
            <w2:dataCollection baseNode="map">
            </w2:dataCollection>
            <w2:workflowCollection>
            </w2:workflowCollection>
        </xf:model>
         <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		
	};
	
	scwin.onpageunload = function() {
		
	};
	
	
	scwin.serviceToggleBtn_onclick = function(e) {
		const stb = serviceToggleBtn;		
		if (stb.hasClass("mobile") || !stb.hasClass("home")){
			stb.removeClass("mobile");
			stb.addClass("home");
			console.log("홈");
		} else if (stb.hasClass("home")){
			stb.removeClass("home");
			stb.addClass("mobile");
			console.log("모바일");
		}
	};
	]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
    	<xf:group style="" id="" class="wrapper">
    		<xf:group style="" id="" class="header" tagname="section">
    			
    		<xf:group style="" id="" class="titlebar">
    				<xf:group id="" class="cta"><w2:textbox tagname="h2" style="" label="가입정보조회" id="" class="title"></w2:textbox></xf:group></xf:group><xf:group tagname="button" style="" id="" class="btn_menu"></xf:group></xf:group>
    		<xf:group style="" id="" class="container vcenter" tagname="section">
    			<xf:group style="" id="" class="container_inner">
    				<xf:group style="" id="" class="searchbox">
    						<xf:group style="" id="serviceToggleBtn" class="toggle_swich mobile" ev:onclick="scwin.serviceToggleBtn_onclick">
    							<xf:group style="" id="" class="bg"></xf:group>
    							<xf:group class="btn" id="" style=""></xf:group>
    							<xf:group class="label" id="" style="">
    								<w2:textbox id="" label="모바일" style="" tagname="span" class="mobile"></w2:textbox>
    								<w2:textbox id="" label="홈" style="" tagname="span" class="home"></w2:textbox>
    							</xf:group>
    						</xf:group>
    						<xf:group id="" style="" class="inputbox">
    							<xf:group id="" class="boxitem">
    								<w2:textbox tagname="label" style="" label="상품번호" id="" class="req"></w2:textbox>
    								<xf:input adjustMaxLength="false" style="" id="" placeholder="01000000000" class="req"></xf:input>
    							</xf:group>
    							<xf:group class="boxitem" id="" style="">
    								<w2:textbox id="" label="고객명" style="" tagname="label"></w2:textbox>
    								<xf:input adjustMaxLength="false" id="" style="" placeholder="홍길동"></xf:input>
    							</xf:group>
    							<xf:group class="boxitem" id="" style="">
    								<w2:textbox id="" label="생년월일" style="" tagname="label"></w2:textbox>
    								<xf:input adjustMaxLength="false" id="" style="" placeholder="1900-00-00"></xf:input>
    							</xf:group>
    						</xf:group>
    						<xf:group id="" style="" class="btnbox">
    							<xf:trigger type="button" style="" id="" class="btn_sh">
    								<xf:label><![CDATA[조회]]></xf:label>
    							</xf:trigger>
    						</xf:group>
    				</xf:group>

    			</xf:group>
    		</xf:group>
    		<xf:group style="" id="" class="bottomMenu" tagname="section">
    			<xf:group style="" id="" class="navbox">
    				<xf:group style="" id="" class="menu01">
    					<xf:group id="" class="numberbox"><w2:textbox style="" label="13" id="" class="num"></w2:textbox></xf:group><w2:textbox class="title" id="" label="To-Do" style=""></w2:textbox>
    				</xf:group>
    				<xf:group class="menu02 on" id="" style="">
    					<w2:textbox class="title" id="" label="가입정보조회" style=""></w2:textbox>
    				</xf:group>
    				<xf:group class="menu03" id="" style="">
    					<w2:textbox class="title" id="" label="사전조회" style=""></w2:textbox>
    				</xf:group>
    				<xf:group class="menu04" id="" style="">
    					<w2:textbox class="title" id="" label="개통지역" style=""></w2:textbox>
    				</xf:group>
    				<xf:group class="menu05" id="" style="">
    					<xf:group class="numberbox" id="" style="">
    						<w2:textbox class="num" id="" label="3" style=""></w2:textbox>
    					</xf:group>
    					<w2:textbox class="title" id="" label="알림" style=""></w2:textbox>
    				</xf:group>
    			</xf:group>
    		</xf:group>
    	</xf:group>
    </body>
</html>
